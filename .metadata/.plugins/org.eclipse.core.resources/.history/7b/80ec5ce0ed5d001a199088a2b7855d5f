DROP TABLE IF EXISTS `pelicula`;
CREATE TABLE `pelicula`
(
    `id` VARCHAR(255)  PRIMARY KEY,
    `title` VARCHAR(255),
    `genre` VARCHAR(255),
    `year` VARCHAR(255),
    `oscars` VARCHAR(255),
);



