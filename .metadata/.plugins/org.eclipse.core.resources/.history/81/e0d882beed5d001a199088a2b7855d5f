DROP TABLE IF EXISTS `pelicula`;
CREATE TABLE `pelicula`
(
    `id` VARCHAR(255)  PRIMARY KEY auto_increment,
    `title` VARCHAR(255),
    `genre` VARCHAR(255),
    `year` VARCHAR(255),
    `oscars` VARCHAR(255)
);



INSERT INTO `pelicula` VALUES ('Star Wars','ficción','1995','0');
INSERT INTO `pelicula` VALUES ('Señor Anillos','ficción','1999','1');
INSERT INTO `pelicula` VALUES ('El Camino','ficción','1985','0');
