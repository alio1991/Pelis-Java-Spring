DROP TABLE IF EXISTS `client`;
CREATE TABLE `client`
(
    `dni` VARCHAR(9) PRIMARY KEY,
    `name` VARCHAR(255) ,
    `last_name` VARCHAR(255),
    `phone` VARCHAR(255)
);

