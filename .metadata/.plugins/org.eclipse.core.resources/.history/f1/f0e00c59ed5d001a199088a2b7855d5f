DROP TABLE IF EXISTS `pelicula`;
CREATE TABLE `pelicula`
(
    `id` VARCHAR(255)  PRIMARY KEY auto_increment,
    `title` VARCHAR(255)  notnul,
    `genre` VARCHAR(255),
    `year` VARCHAR(255),
    `oscars` VARCHAR(255),
);

CREATE TABLE `actor`
(
    `id` VARCHAR(255)  PRIMARY KEY auto_increment,
    `name` VARCHAR(255)  notnul,
    `surname` VARCHAR(255),
);

INSERT INTO `pelicula` VALUES ('Star Wars','100', 'ficción');
INSERT INTO `pelicula` VALUES ('Señor Anillos'    ,'99',  'ficción');
